<li class="event-item">
  <a :href="buildURL(this.event)">
    <time
      :datetime="this.event.values.DateTime"
      @text="formatDate(this.event.values.DateTime, 'event')"
    ></time>
  </a>
  <span
    webc:if="event.values.Notes"
    @html="mdInline(`&ndash; ${this.event.values.Notes}`)"
  ></span>
</li>
